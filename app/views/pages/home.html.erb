<h1>Cats!!</h1>
<% @cats.each do |cat| %>
      <tr>
        <td><%= cat.name %></td>
      </tr>
    <% end %>

<h2>Todos!</h2>
<% @todos.each do |todo| %>
      <tr>
        <td><%= todo.tasks %></td>
        <td><%= todo.finished %></td>
      </tr>
    <% end %>

<h3>Users</h3>
<% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
      </tr>
    <% end %>

<br></br>
<%= form_for @td do |f| %>
  <%= f.label :tasks %>:
  <%= f.text_field :tasks %><br />

  <%= f.label :finished %>:
  <%= f.text_field :finished %><br />
  <%= f.submit %>
<% end %>
